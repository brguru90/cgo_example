#include<stdio.h>
#include <stdarg.h>

typedef struct Values
{
    int a,b;
} values;


typedef void* func_type(void *);


void* a_func(int a,int b,...){
    va_list ptr;
    va_start(ptr, 1);
    if(a==NULL){
        return va_arg(ptr, void*);
    }
    va_end(ptr);

    return a+b;
}

int b_func(int m){
    return m+1;
}

void* with_context(func_type f,int a){
    return a_func(NULL,a,f);
}

int run_callback(func_type f){
    return f(8);
}


int main(){
    int a=10;

    printf("%d\n\n",run_callback(b_func));
    printf("%d\n\n",run_callback(with_context(a_func,a)));


}